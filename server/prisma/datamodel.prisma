type Link {
  id: ID! @id
  createdAt: DateTime! @createdAt
  url: String!
  description: String!
  postedBy: User @relation(name: "PostedByUser")
}

type User {
  id: ID! @id
  name: String!
  email: String! @unique
  password: String!
  links: [Link!]! @relation(name: "PostedByUser", link: INLINE, onDelete: CASCADE)
}
